# Configuración general
PORT=3090
NODE_ENV=development

# Credenciales de AssemblyAI (STT principal)
ASSEMBLYAI_API_KEY=tu_api_key_assemblyai_aqui

# Credenciales de MiniMax
MINIMAX_API_KEY=tu_api_key_aqui
MINIMAX_GROUP_ID=tu_group_id_aqui

# Configuración de MiniMax
MINIMAX_MODEL=speech-02-hd
MINIMAX_VOICE=Friendly_Person

# URLs para la API de MiniMax
MINIMAX_API_URL=https://api.minimax.chat/v1/t2a_v2
MINIMAX_STT_API_URL=https://api.minimax.chat/v1/audio/transcriptions

# Configuración de WhatsApp
ENABLE_WHATSAPP=true
WHATSAPP_SESSION_NAME=bot
WHATSAPP_PROVIDER=baileys

# Configuración de Supabase
ENABLE_SUPABASE=false
SUPABASE_URL=https://tu-proyecto.supabase.co
SUPABASE_ANON_KEY=tu_anon_key_aqui
SUPABASE_SERVICE_KEY=tu_service_key_aqui

# Configuración multitenant
ENABLE_MULTITENANT=false
DEFAULT_TENANT_ID=default

# Configuración de caché
ENABLE_TTS_CACHE=true
TTS_CACHE_EXPIRATION=86400
